<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>


<h3>Track your tickers!</h3>
<form action="/tickers" method="POST">
    <div class="form-group col-md-4">
      <input type="text" class="form-control" name="tickerfield1" placeholder="Select Ticker #1">
    </div>
    <div class="form-group col-md-4">
      <input type="text" class="form-control" name="tickerfield2" placeholder="Select Ticker #2">
    </div>
    <div class="form-group col-md-4">
      <button type="Submit" class="btn btn-primary col-md-12">Graph It!</button>
    </div>
</form>


